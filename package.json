{
    "name": "obsidian-auto-tagger",
    "type": "module",
    "version": "1.0.0",
    "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
    "main": "main.js",
    "scripts": {
        "dev": "pnpm dlx tsx esbuild.config.mts",
        "build": "pnpm dlx tsx esbuild.config.mts production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "check-types": "tsc -noEmit -skipLibCheck",
        "repomix": "pnpm dlx repomix && code ./repomix-output.md",
        "setup-symlink": "node scripts/setup-symlink.mjs",
        "rename-plugin": "node scripts/rename-plugin.mjs",
        "format": "pnpm dlx prettier --write .",
        "test:e2e": "playwright test",
        "test:e2e:example": "pnpm playwright test e2e/specs/example.spec.ts --project=chromium",
        "test:e2e:debug": "cross-env DEBUG=pw:api playwright test"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "loglevel": "^1.9.2",
        "svelte": "4.2.20"
    },
    "devDependencies": {
        "@electron/asar": "^4.0.1",
        "@playwright/test": "^1.56.0",
        "@tsconfig/svelte": "5.0.5",
        "@types/node": "24.3.1",
        "builtin-modules": "5.0.0",
        "chalk": "^5.6.2",
        "dotenv": "^17.2.2",
        "electron": "^38.2.2",
        "esbuild": "0.25.9",
        "esbuild-svelte": "^0.9.3",
        "loglevel-plugin-prefix": "^0.8.4",
        "obsidian": "^1.8.7",
        "obsidian-typings": "4.50.0",
        "playwright": "^1.56.0",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "svelte-preprocess": "^6.0.3",
        "tiny-invariant": "^1.3.3",
        "tslib": "^2.8.1",
        "typescript": "5.9.2",
        "vitest": "^3.2.4"
    }
}
